{
  "name": "showcase",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ronuse/noseur": "file:../ronuse-noseur-1.0.0.tgz",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:noseur": "cd ../ & npm run pack & cd showcase",
    "clean:start": "rm -Rf node_modules/.cache & npm uninstall @ronuse/noseur & cls & npm install ../ronuse-noseur-1.0.0.tgz & npm start"
  },
  "watch": {
    "clean:start": {
      "patterns": [
        "../_src",
        "../ronuse-noseur-*.tgz"
      ],
      "extensions": "ts,tsx,scss,tgz"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-watch": "^0.11.0"
  }
}
